








<table class="table table-hover">
    <thead>
    <th>Usuario</th>
    <th>Nª Personas</th>
    <th>Fecha</th>
    <th>Hora</th>
    <th>Mesa</th>
    <th>Detalle Pedido</th>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Usuario</td>
            <td>@item.N_personas</td>
            <td>@item.Fecha</td>
            <td>@item.Hora</td>
            <td>@item.Mesa</td>
            <td><a href="https://localhost:44348/reserva/detallepedido" class="btn btn-success">Detalle Pedido</a></td>
        </tr>

        }
    </tbody>
</table>
